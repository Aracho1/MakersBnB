    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
    <link rel="stylesheet" href="stylesheets/style.css">
  </head>
  <body>

<div class="banner_light">

  <nav class="navbar navbar-dark bg-transparent">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar">Sign up</span>
        <span class="icon-bar">Login</span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="/"><i class="fab fa-airbnb"></i> MakersBnB</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
          <li><a href="/register">Sign up</a></li>
          <li><a href="/login">Login</a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
    <form class="form-inline my-2 my-lg-0">
      <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
      <button class="search_button" type="submit">Search</button>
    </form>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav> 

<h1><span>MakersBnB.</span> List your space.</h1>

</div>

<div class="listing_form_container text-center">
  <div class="card text-center" style="width: 40%">
    <form action='/list_new_room' method='post'  enctype="multipart/form-data">
    <h4> Name </h4><input type='text' name='name'></input>
    <h4> Price </h4><input type='number' min="0" name='price'></input>
    <h4> Description </h4>
      <input type='text' name='description'></input>
    <h4> Available from </h4>
      <input type='date' name='available_from'></input>
    <h4> Available to </h4>
      <input type='date' name='available_to'></input>
    <h4>Upload Image</h4>
      <center><input type="file" name="image" class="inputfile"></center>
    <br>
      <input type='submit' value='List Room'>
    <p>
    </form>
  </div>
  <p>
<footer>MakersBnB 2021. All rights reserved.</footer>
</div>
</body>
</html>
